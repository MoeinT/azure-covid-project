                            "name": "age_group_25_49",
                            "type": "String"
                        },
                        "sink": {
                            "name": "age_group_25_49"
                        }
                    },
                    {
                        "source": {
                            "name": "age_group_50_64",
                            "type": "String"
                        },
                        "sink": {
                            "name": "age_group_50_64"
                        }
                    },
                    {
                        "source": {
                            "name": "age_group_65_79",
                            "type": "String"
                        },
                        "sink": {
                            "name": "age_group_65_79"
