                        }
                    },
                    {
                        "source": {
                            "name": "country_code_3_digit",
                            "type": "String"
                        },
                        "sink": {
                            "name": "country_code_3_digit"
                        }
                    },
                    {
                        "source": {
                            "name": "population",
                            "type": "String"
                        },
                        "sink": {
                            "name": "population"
                        }
                    },
                    {
                        "source": {
